<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs') %>

<body>
    <%- include('./partials/nav.ejs') %>

    <div class="items-container">
        <div class="items-list">
          <h2>Foods</h2>

          <% const foodArray = [39, 40, 282, 295, 296, 297, 260, 322, 391, 396, 392, 393, 394, 413, 434, 435, 436, 319, 349, 363, 365, 411, 423, 320, 350, 364, 366, 412, 424] %>
          
          <% const foodFilteredArray = items.filter((item) => { %>
            <% return foodArray.some((f) => { %>
              <% return f === item.type; %>
            <% }); %>
          <% }) %> 

          <% foodFilteredArray.forEach((item) => { %>
          <p class="item"><%= item.name %></p>
          <% }) %>
        </div>
      </div>

</body>
</html>